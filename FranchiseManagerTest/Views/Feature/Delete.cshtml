@model FranchiseManagerTest.Models.Feature

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>Feature</legend>

    <div class="display-label">FeatureName</div>
    <div class="display-field">@Model.FeatureName</div>

    <div class="display-label">FranchiseSets</div>
    <div class="display-field">@(Model.FranchiseSets == null ? "None" : Model.FranchiseSets.Count.ToString())</div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}


